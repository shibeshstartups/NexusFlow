# Development Environment Variables for NexusFlow
# This file should be kept in version control

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_CDN_BASE_URL=http://localhost:3000/cdn
VITE_STORAGE_BASE_URL=http://localhost:3000/storage

# Authentication
VITE_JWT_SECRET=dev-jwt-secret-change-in-production
VITE_JWT_EXPIRES_IN=7d
VITE_REFRESH_TOKEN_EXPIRES_IN=30d

# OAuth Configuration (Development keys)
VITE_GOOGLE_CLIENT_ID=your-dev-google-client-id
VITE_GOOGLE_CLIENT_SECRET=your-dev-google-client-secret

# Payment Gateway (Test mode)
VITE_RAZORPAY_KEY_ID=rzp_test_your_test_key_id
VITE_RAZORPAY_KEY_SECRET=your-test-key-secret
VITE_RAZORPAY_WEBHOOK_SECRET=your-test-webhook-secret

# Monitoring & Analytics (Development)
VITE_SENTRY_DSN=your-dev-sentry-dsn
VITE_SENTRY_ENVIRONMENT=development
VITE_SENTRY_RELEASE=dev
VITE_LOGROCKET_APP_ID=your-dev-logrocket-app-id
VITE_GOOGLE_ANALYTICS_ID=your-dev-analytics-id

# Storage Configuration (Development)
VITE_S3_BUCKET_NAME=nexusflow-development
VITE_S3_REGION=ap-south-1
VITE_S3_ACCESS_KEY_ID=your-dev-s3-access-key
VITE_S3_SECRET_ACCESS_KEY=your-dev-s3-secret-key
VITE_R2_ACCOUNT_ID=your-dev-r2-account-id
VITE_R2_ACCESS_KEY_ID=your-dev-r2-access-key
VITE_R2_SECRET_ACCESS_KEY=your-dev-r2-secret-key

# CDN Configuration
VITE_CLOUDFLARE_ZONE_ID=your-dev-zone-id
VITE_CLOUDFLARE_API_TOKEN=your-dev-api-token

# Database
VITE_MONGODB_URI=mongodb://localhost:27017/nexusflow-dev
VITE_REDIS_URL=redis://localhost:6379

# Email Service
VITE_EMAIL_SERVICE_API_KEY=your-dev-email-api-key
VITE_EMAIL_FROM=dev@nexusflow.in

# App Configuration
VITE_APP_NAME=NexusFlow (Dev)
VITE_APP_URL=http://localhost:5173
VITE_APP_VERSION=1.0.0-dev
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_MONITORING=true
VITE_ENABLE_PWA=false
VITE_ENABLE_OFFLINE_MODE=false

# Performance
VITE_ENABLE_CDN=false
VITE_ENABLE_COMPRESSION=false
VITE_ENABLE_CACHING=false

# Security
VITE_ENABLE_CSP=false
VITE_ENABLE_HTTPS_ONLY=false
VITE_ENABLE_CORS=true